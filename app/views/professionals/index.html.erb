
<table>
  <% @professionals.each do |professional| %>
    <tr>
      <td><%= link_to professional.nombre, professional_path(professional) %></td>
      <td><%= button_to 'Edit', edit_professional_path(professional), method: 'get' %></td>
      <td><%= button_to 'remove', professional_path(professional), method: 'delete', data: {confirm: 'Are you sure?'} %></td>
  <% end %>
</table>
<%= link_to "Add a New Professional", new_professional_path %>
